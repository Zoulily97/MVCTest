@model BLL.Entities.Student 
<h1>学生登录index</h1>
@using (Html.BeginForm())
{

    <label>邀请人</label>
    @Html.TextBoxFor(m => m.InvitedBy)
    <label>姓名</label>
    @Html.TextBoxFor(m => m.Name)
    @Html.ValidationMessageFor(m => m.Name)
    <label>ID</label>
    @Html.TextBoxFor(m => m.Id)
    <label>密码</label>
    @Html.TextBoxFor(m => m.Password)
    <label>验证密码</label>
    @Html.TextBoxFor(m => m)
}